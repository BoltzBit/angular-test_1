{
  "name": "angular-test-1",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-common": "ng test --watch=false --browsers Chrome,Firefox",
    "test-ci": "ng test --watch=false --reporters junit --browsers ChromeHeadless,FirefoxDescabecado",
